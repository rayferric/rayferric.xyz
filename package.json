{
  "name": "rayferric.xyz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker-compose up dev",
    "deploy": "docker-compose up -d --build deploy",
    "stop": "docker-compose down",
    "format": "prettier --write . !./data",
    "postgres-dump": "docker-compose up -d postgres && sleep 3 && docker-compose exec postgres sh -c 'rm -rf /mount/dump/* && pg_dump --dbname=$POSTGRES_DB --username=$POSTGRES_USER --exclude-table-data=sessions -Z0 -j 8 -Fd -f /mount/dump'",
    "postgres-restore": "docker-compose up -d postgres && sleep 3 && docker-compose exec postgres sh -c 'psql -U $POSTGRES_USER -d $POSTGRES_DB -c \"DROP SCHEMA IF EXISTS public CASCADE; CREATE SCHEMA public;\" && pg_restore --dbname=$POSTGRES_DB --username=$POSTGRES_USER -j 8 -Fd -O /mount/dump'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/mersenne-twister": "^1.1.2",
    "@types/minio": "^7.0.15",
    "@types/node": "18.11.13",
    "@types/pg": "^8.6.5",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/react-twemoji": "^0.4.0",
    "@types/sha.js": "^2.4.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "mersenne-twister": "^1.1.0",
    "minio": "^7.0.32",
    "next": "13.0.6",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.43",
    "pg": "^8.8.0",
    "prettier": "^2.8.1",
    "pretty-bytes": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.4",
    "react-spinners": "^0.13.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-twemoji": "^0.5.0",
    "rehype-raw": "^6.1.1",
    "sha.js": "^2.4.11",
    "typescript": "4.9.4"
  }
}
